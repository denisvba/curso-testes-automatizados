{
  "name": "aula-1",
  "version": "1.0.0",
  "description": "Aula 1 - Fundamentos de automação de testes e scripts",
  "main": "circle.js",
  "scripts": {
    "test": "jest --verbose --coverage",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denisvba/curso-testes-automatizados.git"
  },
  "author": "Flávia Béo - flavia.beo@ibm.com, Gabriela Moreira - @gabiborbolato, Denis Almeida - @denisvba",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/denisvba/curso-testes-automatizados/issues"
  },
  "homepage": "https://denisvba.github.io/curso-testes-automatizados",
  "devDependencies": {
    "jest": "^27.4.7"
  },
  "dependencies": {
    "gh-pages": "^3.2.3",
    "jest-stare": "^2.3.0",
    "react-scripts": "^5.0.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/Aula 1",
      "<rootDir>/Aula 3",
      "<rootDir>/Aula 4",
      "<rootDir>/Aula 5"
    ],
    "collectCoverage": true,
    "coverageReport": [
